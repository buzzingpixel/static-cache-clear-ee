{
    "name": "buzzingpixel/static-cache-clear-ee",
    "version": "1.0.0",
    "description": "Clear static cache files in ExpressionEngine",
    "type": "ee-add-on",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:tjdraper/expressionengine.git"
        }
    ],
    "require-dev": {
        "tjdraper/expressionengine": "^4.0.8",
        "tjdraper/executive-ee": "^2.0.4",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "buzzingpixel\\cacheclear\\": "system/user/addons/static_cache_clear",
            "EllisLab\\" : "vendor/tjdraper/expressionengine/src/system/ee/EllisLab"
        }
    },
    "extra": {
        "handle": "static_cache_clear",
        "systemPath": "static_cache_clear"
    }
}
